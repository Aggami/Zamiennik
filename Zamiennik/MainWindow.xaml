<Window x:Class="Zamiennik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zamiennik"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800"
        Style="{StaticResource CustomWindowStyle}">
    <UniformGrid>
        <Button x:Name="istniejace" Style="{StaticResource decisionButtons}" Margin="5" Content="Wyszukaj istniejący zamiennik" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" Click="Button_Click" />
        <Button x:Name="propozycje" Style="{StaticResource decisionButtons}" Margin="5" Content="Rozpatrz propozycję zamiennika" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="Button_Click_1" />

    </UniformGrid>
</Window>
